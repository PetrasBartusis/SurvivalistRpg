          <argument>
            <kind>1</kind>
            <string>/// Control inventory's depth
depth = -y - 2000;
//move inventory so the player can see it
if(instance_exists(obj_player)){
    x = view_xview[0];
    y = view_yview[0];
}

if(ds_grid_get(myItems, 0, 0) != 0){
    isEmpty = false;
    //F = ds_grid_height(myItems);
    //if(ds_grid_height(myItems) &gt;= floor((sprite_height - (textBorder * 3) / 16 + 100))){
        //inventoryEndAt = floor(sprite_height - (textBorder * 3) / 16);
    //}
    inventoryEndAt = min(ds_grid_height(myItems), floor((sprite_height - (textBorder * 3)) / 16));
}

