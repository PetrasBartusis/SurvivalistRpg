        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Draw the inventory on screen
draw_self();

draw_set_colour(myColor);
draw_set_font(inventory_font);
draw_text(bbox_left + textBorder, bbox_top + textBorder, "Image");
draw_text(bbox_left + 60, bbox_top + textBorder, "Name");
draw_text(bbox_left + 100, bbox_top + textBorder, "Amount");

itemLeftStart = bbox_left + 60;
itemTopStart = bbox_top + 36;

for(i = 0; i &lt; inventoryEndAt; i++){
    for(j = 0; j &lt; playerInventoryWidth; j++){
        inventoryOnScreen = i;
        if(j == 0){
            draw_text(itemLeftStart, itemTopStart + (i * 16), ds_grid_get(myItems, 0, i + scrolledAmount));
        } else if(j == 1){
            draw_text(itemLeftStart + 58, itemTopStart + (i * 16), ds_grid_get(myItems, 1, i + scrolledAmount));
        } else if(j == 3){
            draw_sprite(ds_grid_get(myItems, j, i + scrolledAmount), 0, bbox_left + 40, itemTopStart + (i * 16) + 8);
        }
    }
}

draw_rectangle(bbox_left + textBorder, itemTopStart + ((itemSelected - scrolledAmount) * 16), 
bbox_right - textBorder, itemTopStart + ((itemSelected - scrolledAmount) * 16) + 16, true);

draw_sprite(spr_item_box, 0, bbox_right, bbox_top);

if(isEmpty){
    draw_text_ext(bbox_right+16, bbox_top+16, emptyMessage, 16, sprite_get_width(spr_item_box) - textBorder);
} else {
    draw_text_ext(bbox_right+16, bbox_top+16, ds_grid_get(myItems, 2, itemSelected), 10, sprite_get_width(spr_item_box) - textBorder);
}

if(ds_grid_height(myItems) &gt; floor((sprite_height - (textBorder * 3)) / 16)){
    draw_sprite(spr_scroll_bar, 0, bbox_right, 10 + bbox_top + itemSelected * (sprite_height - textBorder) / ds_grid_height(myItems));
}
</string>
          </argument>
