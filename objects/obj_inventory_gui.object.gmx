            <string>/// Initialize the player's inventory's GUI
textBorder = 22;
myItems = playerInventory;
myColor = c_black;
isEmpty = false;
emptyMessage = "You have no items.";

globalvar itemSelected, scrolledAmount, inventoryEndAt;
itemSelected = 0;
scrolledAmount = 0;
inventoryEndAt = min(ds_grid_height(myItems), floor((sprite_height - (textBorder * 3)) / 16));
inventoryOnScreen = 0;
